# OpenRouter API key (used for both RAG agent and ingestion pipeline)
OPENROUTER_API_KEY=<your-openrouter-api-key>

# Firecrawl API key (for web crawling in ingestion pipeline)
FIRECRAWL_API_KEY=<your-firecrawl-api-key>

# Cohere API key (for embeddings in ingestion pipeline)
COHERE_API_KEY=<your-cohere-api-key>

# Weaviate Cloud credentials (for vector database)
WEAVIATE_URL=<your-weaviate-cloud-url>
WEAVIATE_API_KEY=<your-weaviate-api-key>

# LangSmith credentials (for tracing and monitoring)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=<your-langsmith-api-key>
LANGCHAIN_PROJECT=<your-project-name>
